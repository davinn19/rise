{
	"manifest_version": 2,
	"name": "Rise",
	"description": "Simple animated home and new tab screen",
	"version": "0.0.1",
	"icons": {
		"64": "icons/icon.png"
	},
	"browser_action": {
		"default_icon": {
			"64": "icons/icon.png"
		},
		"default_popup": "browserAction/index.html",
		"default_title": "Rise"
	},
	"chrome_url_overrides" : {
		"newtab": "newTab/index.html"
	}
}